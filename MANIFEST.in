# Tells "python setup.py sdist" what files to include in the tarball.

include .coveragerc
include .git-pre-commit
include CREDITS
include HISTORY.rst
include LICENSE
include make.bat
include Makefile
include MANIFEST.in
include README.rst
recursive-include demo *.py *.pem *README*
recursive-include pyftpdlib *.py *.pem *README*
recursive-include scripts *
recursive-exclude docs/_build *
recursive-include docs *.rst *.js *.html *.py *.bat *Makefile* *README*
